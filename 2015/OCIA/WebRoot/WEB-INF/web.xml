<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>LoginAction</servlet-name>
    <servlet-class>login.LoginAction</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>RegisterAction</servlet-name>
    <servlet-class>register.RegisterAction</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>DataResponse</servlet-name>
    <servlet-class>ocia.register.DataResponse</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Tests</servlet-name>
    <servlet-class>register.Tests</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>DataResponseL</servlet-name>
    <servlet-class>ocia.login.DataResponseL</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UpdatePersonalInfo</servlet-name>
    <servlet-class>login.UpdatePersonalInfo</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RegisterActionNoOcia</servlet-name>
    <servlet-class>register.RegisterActionNoOcia</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>LoginAction</servlet-name>
    <url-pattern>/servlet/LoginAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegisterAction</servlet-name>
    <url-pattern>/servlet/RegisterAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DataResponse</servlet-name>
    <url-pattern>/servlet/DataResponse</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Tests</servlet-name>
    <url-pattern>/servlet/Tests</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>DataResponseL</servlet-name>
    <url-pattern>/servlet/DataResponseL</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UpdatePersonalInfo</servlet-name>
    <url-pattern>/servlet/UpdatePersonalInfo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegisterActionNoOcia</servlet-name>
    <url-pattern>/servlet/RegisterActionNoOcia</url-pattern>
  </servlet-mapping>
</web-app>